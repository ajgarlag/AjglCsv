{
    "name": "ajgl/csv",
    "description": "Classes for reading and writing CSV files (includes RFC 4180 compliant classes)",
    "keywords": ["csv"],
    "homepage": "https://github.com/ajgl/AjglCsv",
    "license": "MIT",
    "authors": [
      {
        "name": "Antonio J. GarcÃ­a Lagar",
        "email": "aj@garcialagar.es",
        "homepage": "http://aj.garcialagar.es",
        "role": "developer"
      }
    ],
    "autoload": {
        "psr-0": {
            "Ajgl": "src/"
        }
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.phpunit.de"
        },
        {
            "type": "vcs",
            "url": "http://github.com/ajgarlag/PHP_CodeSniffer"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phing/phing": "dev-master",
        "apigen/apigen": "*",
        "pdepend/pdepend": "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "dev-composer",
        "pear-pear.phpunit.de/phpcpd": "*",
        "pear-pear.phpunit.de/phploc": "*",
        "pear-pear.phpunit.de/PHP_CodeBrowser": "*"
    },
    "suggest": {
        "ext-iconv": "Enabling the iconv extension allows you to convert the string using the IconvConverter class"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
